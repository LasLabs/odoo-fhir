        <record id="hc_substance.subs_cat_allergen" model="hc.vs.substance.category">   
            <field name="code">allergen</field>
            <field name="name">Allergen</field>
            <field name="definition">A substance that causes an allergic reaction.</field>
            <field name="snomed_code">406455002</field>
            <field name="snomed_definition">Allergen class (substance)</field>
            <field name="source_id" ref="base.hl7fhir"/>
            <field name="system">http://hl7.org/fhir/ValueSet/substance-category</field>
            <field name="version"></field>
        </record>   
            
        <record id="hc_substance.subs_cat_biological" model="hc.vs.substance.category"> 
            <field name="code">biological</field>
            <field name="name">Biological Substance</field>
            <field name="definition">A substance that is produced by or extracted from a biological source.</field>
            <field name="snomed_code">115668003</field>
            <field name="snomed_definition">Biological substance (substance)</field>
