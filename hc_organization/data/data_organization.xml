            <field name="source_id" ref="base.hl7fhir"/>
            <field name="system">http://hl7.org/fhir/ValueSet/contactentity-type</field>
        </record>   
            
        <record id="hc_organization.cont_ent_type_press" model="hc.vs.contact.entity.type"> 
            <field name="code">PRESS</field>
            <field name="name">Press</field>
            <field name="definition">Dedicated contact point for matters relating to press enquiries.</field>
            <field name="source_id" ref="base.hl7fhir"/>
            <field name="system">http://hl7.org/fhir/ValueSet/contactentity-type</field>
        </record>   

    </data>
